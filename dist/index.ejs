            <% }; %>
            <% if(model.workRole3 !== ""){ %>
            <p id="job-title"><strong><%= model.workRole3 %></strong></p>
            <% }; %>
            <p id="job-responsibilities">Job Responsbilities</p>
            <p>
            <ul>
                <% if(model.workDuties3 !== ""){ %>
                <li><%= model.workDuties3 %></li>
                <% }; %></ul>
            </p>
            <h3>Technical Skills</h3>
            <p>
            <ul>
                
                 <% if(model.skill1 !==""){ %>
                <li><span id="course-name"><%= model.skill1 %></span></li>
                <% }; %>
                 <% if(model.skill2 !==""){ %>
                <li><span id="course-name"><%= model.skill2 %></span></li>
                <% }; %>
            </ul>
            </p>
            <h3>Certifications / Awards:</h3>
            <p>
            <ul>
                <% if(model.certificateName1 !=="" && model.schoolName1 !== "" && model.optentiondate1 !==""){ %>
                <li><%= model.certificateName1 %> at <%= model.schoolName1 %> in <%= model.optentiondate1 %></li>
                <% }; %>
                <% if(model.certificateName2 !=="" && model.schoolName2 !== "" && model.optentiondate2 !==""){ %>
                <li><%= model.certificateName2 %> at <%= model.schoolName2 %> in <%= model.optentiondate2 %></li>
                <% }; %>
                <% if(model.certificateName3 !=="" && model.schoolName3 !== "" && model.optentiondate3 !==""){ %>
                <li><%= model.certificateName3 %> at <%= model.schoolName3 %> in <%= model.optentiondate3 %></li>
                <% }; %> </ul>
            </p>
            <h3>Language</h3>

            <p>
            <ul>
                 <% if(model.langue1 !==""){ %>
                <li><%= model.langue1 %></li>
                <% }; %>
                  <% if(model.langue2 !==""){ %>
