            <% }; %>
            <% if(model.workRole1 !== ""){ %>
            <p id="job-title"><strong><%= model.workRole1 %></strong></p>
            <% }; %>
            <p id="job-responsibilities">Job Responsbilities</p>
            <p>
            <ul>
                <% if(model.workDuties1 !== ""){ %>
                <li><%= model.workDuties1 %></li>
                <% }; %></ul>
            </p>
             <% if(model.workPlace2 !== ""){ %>
            <h4 id="company-name"><%= model.workPlace2 %></h4>
            <% }; %>
