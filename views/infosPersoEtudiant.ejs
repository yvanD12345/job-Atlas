            <%=user.Age%>
        </label>
        <br>
        <h3 style="display:inline-block;">Email</h3>
        <label for="emailEtudiant" style="display:inline-block;">
            <%=user.email%>
        </label>
        <br>
        <h4 style="display:inline-block;">Mot de passe</h4>
        <label for="mdpEtudiant" style="display:inline-block;">
            <%=user.password%>
